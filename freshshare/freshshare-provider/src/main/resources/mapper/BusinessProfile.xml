<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.freshshare.mapper.PProfileMapper">


    <select id="selectProfile" parameterType="com.freshshare.controller.Dto.ProfileDto">
        SELECT
            *
        FROM
            business
        where
            business_id = #{business_id}
    </select>




</mapper>